{
  "name": "@curiousyuvi/tquery-actions",
  "version": "1.0.9",
  "description": "A library of TanStack Query actions.",
  "author": "curiousyuvi",
  "license": "Apache-2.0",
  "repository": "curiousyuvi/tquery-actions",
  "main": "dist/index.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
